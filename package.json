{
    "name": "flexlm-license-file",
    "displayName": "flexlm license file",
    "description": "Syntax highlight for FlexLM license file",
    "version": "0.1.0",
    "publisher": "sekineh",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "flexlm-lic",
            "aliases": ["FlexLM License File", "flexlm-lic"],
            "extensions": [".lic"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "flexlm-lic",
            "scopeName": "source.flexlm",
            "path": "./syntaxes/flexlm-lic.tmLanguage.json"
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sekineh/vscode-flexlm-license-file"
    }
}